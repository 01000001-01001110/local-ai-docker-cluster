# PostgreSQL Configuration
POSTGRES_USER=root
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=n8n

# MongoDB Configuration
MONGODB_USER=root
MONGODB_PASSWORD=your_secure_password
MONGODB_DATABASE=langchain_db

# n8n Configuration
N8N_ENCRYPTION_KEY=your_encryption_key_min_32_chars
N8N_USER_MANAGEMENT_JWT_SECRET=your_jwt_secret_min_32_chars

# Optional: Timezone configuration
TZ=UTC

# Note: Replace all "your_secure_password" values with strong, unique passwords
# Generate secure keys for N8N_ENCRYPTION_KEY and N8N_USER_MANAGEMENT_JWT_SECRET
# Minimum length for these keys is 32 characters
